{
  "name": "extract.js",
  "version": "0.5.0",
  "description": "Check or get nested properties on objects in javascript",
  "main": "lib/index.js",
  "scripts": {
    "build": "node_modules/.bin/babel src/ --out-dir lib/",
    "test": "node_modules/.bin/mocha --require node_modules/babel-core/register ./test/",
    "cover": "node_modules/.bin/istanbul cover --report lcov --dir=test/coverage node_modules/.bin/_mocha --require node_modules/babel-core/register ./test/",
    "coveralls": "npm run cover -- --report lcovonly && cat ./test/coverage/lcov.info | coveralls",
    "lint": "node_modules/.bin/eslint -c .eslintrc src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Knape/extract.js.git"
  },
  "keywords": [],
  "author": "Philip Knape <philip.knape@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Knape/extract.js/issues"
  },
  "homepage": "https://github.com/Knape/extract.js#readme",
  "devDependencies": {
    "babel": "~6.5.2",
    "babel-cli": "^6.7.7",
    "babel-core ": "^6.8.0",
    "babel-eslint": "^6.0.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-3": "^6.17.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.16",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-react": "^4.2.3",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^2.4.5",
    "mocha-lcov-reporter": "^1.3.0"
  }
}
